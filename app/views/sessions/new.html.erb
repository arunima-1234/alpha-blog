
<h1> Log in </h1>


<%= form_with(scope: :session, url: login_path, local: true) do |form| %>

  <p><%= form.label :Email %>
    <%= form.email_field :email, placeholder: "Enter your email" %>
  </p>

  <p><%= form.label :Password %>
    <%= form.password_field :password, placeholder: "Enter your password" %>
  </p>


  <p><%= form.submit "Log in" %>
  </p>


<% end %>

